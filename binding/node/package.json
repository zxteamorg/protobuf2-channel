	"license": "MIT",
	"devDependencies": {
		"@types/node": "^11.15.2",
		"@zxnode/build": "^0.0.34",
		"ts-protoc-gen": "^0.12.0"
	},
	"dependencies": {
		"@types/google-protobuf": "^3.7.2",
		"google-protobuf": "^3.10.0"
	},
	"engines": {
		"node": ">=11"
	},
	"keywords": [],
	"repository": {
		"type": "git",
